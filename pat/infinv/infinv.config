{
  "scripts": [ "/pat/infinv/InfiniteInventory.lua" ],
  "scriptDelta": 1,
  "scriptWidgetCallbacks": [
    "TabList._selectCallback",
    "IconPicker._slotLeft",
    "IconPicker._slotRight",
    "Callbacks.newTabButton",
    "Callbacks.moveTabButton",
    "Callbacks.deleteTabButton",
    "Callbacks.sortButton",
    "Callbacks.quickStackbutton",
    "Callbacks.tabConfigCheckbox",
    "Callbacks.tabIconSelected",
    "Callbacks.tabIconRotateButton",
    "Callbacks.tabLabelTextbox",
    "Callbacks.tabLabelBlur",
    "Callbacks.changePage",
    "Callbacks.quickMoveCheckbox"
  ],

  "strings": {
    "tooltips": {
      "newTabButton": "Create New Bag",
      "tabConfigCheckbox": "Edit Bag",
      "quickStackButton": "Quick Stack",
      "sortButton": "Sort Items",
      "gridLayout.prevPageButton": "Previous Page",
      "gridLayout.nextPageButton": "Next Page",
      "editorLayout.settings.moveTabUpButton": "Move Bag Up",
      "editorLayout.settings.moveTabDownButton": "Move Bag Down",
      "editorLayout.settings.deleteTabButton": "Delete Empty Bag",
      "editorLayout.settings.rotateIconButton": "Rotate Icon",
      "quickMoveCheckbox": "Toggle Quick-moving",
      "tabIconSlot": "Insert item to use as icon"
    },
    "title": "Infiniter Inventory",
    "defaultTabSubtitle": "Bag #%s",
    "pageText": "^shadow;%s/%s"
  },

  "gui": {
    "background": {
      "type": "background",
      "fileHeader": "/pat/infinv/images/header.png",
      "fileBody": "/pat/infinv/images/body.png",
      "fileFooter": "/pat/infinv/images/footer.png"
    },
    "title": {
      "type": "title",
      "icon": {
        "type": "image",
        "drawables": [
          { "image": "/interface/inventory/portrait.png", "centered": false },
          { "image": "/pat/infinv/images/icon.png", "centered": false, "position": [ 2, 2 ] }
        ]
      }
    },
    "close": {
      "type": "button",
      "position": [ 234, 147 ],
      "pressedOffset": [ 0, 0 ],
      "base": "/interface/x.png",
      "hover": "/interface/xhover.png",
      "pressed": "/interface/xpress.png"
    },
    "newTabButton": {
      "type": "button",
      "callback": "Callbacks.newTabButton",
      "position": [ 7, 2 ],
      "pressedOffset": [ 0, 0 ],
      "base": "/pat/infinv/images/buttons/plus.png:base",
      "hover": "/pat/infinv/images/buttons/plus.png:base?multiply=aff",
      "pressed": "/pat/infinv/images/buttons/plus.png:press?multiply=0ff"
    },
    "tabConfigCheckbox": {
      "type": "button",
      "checkable": true,
      "callback": "Callbacks.tabConfigCheckbox",
      "position": [ 21, 2 ],
      "pressedOffset": [ 0, 0 ],
      "base": "/pat/infinv/images/buttons/edit.png:base",
      "hover": "/pat/infinv/images/buttons/edit.png:base?multiply=faf",
      "pressed": "/pat/infinv/images/buttons/edit.png:press?multiply=f0f",
      "baseImageChecked": "/pat/infinv/images/buttons/edit.png:press?multiply=faf",
      "hoverImageChecked": "/pat/infinv/images/buttons/edit.png:press?multiply=f0f",
      "pressedImageChecked": "/pat/infinv/images/buttons/edit.png:base"
    },
    "quickStackButton": {
      "type": "button",
      "callback": "Callbacks.quickStackbutton",
      "position": [ 218, 2 ],
      "pressedOffset": [ 0, 0 ],
      "base": "/pat/infinv/images/buttons/quickstack.png:base",
      "hover": "/pat/infinv/images/buttons/quickstack.png:base?multiply=ffa",
      "pressed": "/pat/infinv/images/buttons/quickstack.png:press?multiply=ff0"
    },
    "sortButton": {
      "type": "button",
      "callback": "Callbacks.sortButton",
      "position": [ 232, 2 ],
      "pressedOffset": [ 0, 0 ],
      "base": "/pat/infinv/images/buttons/sort.png:base",
      "hover": "/pat/infinv/images/buttons/sort.png:base?multiply=faa",
      "pressed": "/pat/infinv/images/buttons/sort.png:press?multiply=f00"
    },
    "quickMoveCheckbox": {
      "type": "button",
      "checkable": true,
      "callback": "Callbacks.quickMoveCheckbox",
      "position": [ 55, 2 ],
      "pressedOffset": [ 0, 0 ],
      "base": "/pat/infinv/images/buttons/quickmove.png:base",
      "hover": "/pat/infinv/images/buttons/quickmove.png:base?multiply=aff",
      "pressed": "/pat/infinv/images/buttons/quickmove.png:press?multiply=0ff",
      "baseImageChecked": "/pat/infinv/images/buttons/quickmove.png:press?multiply=aff",
      "hoverImageChecked": "/pat/infinv/images/buttons/quickmove.png:press?multiply=0ff",
      "pressedImageChecked": "/pat/infinv/images/buttons/quickmove.png:base"
    },
    "tabs": {
      "type": "scrollArea",
      "rect": [ 3, 15, 46, 140 ],
      "children": {
        "list": {
          "type": "list",
          "callback": "TabList._selectCallback",
          "scissoring": false,
          "data": {
            "initChildren": [ "button" ],
            "defaultIconMaxIndex": 9,
            "iconTemplate": {
              "type": "image",
              "position": [ 16, 8 ],
              "maxSize": [ 30, 18 ],
              "zlevel": 2,
              "centered": true,
              "mouseTransparent": true
            }
          },
          "schema": {
            "selectedBG": "/pat/infinv/images/tabselectedbg.png",
            "spacing": [ 0, 1 ],
            "memberSize": [ 32, 16 ],
            "listTemplate": {
              "background": {
                "type": "image",
                "zlevel": -10
              },
              "button": {
                "type": "button",
                "callback": "null",
                "position": [ 0, -1 ],
                "pressedOffset": [ 0, -1 ],
                "base": "/pat/infinv/images/tabbutton.png:base",
                "hover": "/pat/infinv/images/tabbutton.png:hover",
                "pressed": "/pat/infinv/images/tabbutton.png:pressed",
                "disabledImage": "/pat/infinv/images/tabbutton.png:disabled"
              }
            }
          }
        }
      }
    },
    "gridLayout": {
      "type": "layout",
      "layoutType": "basic",
      "scissoring": false,
      "children": {
        "slots": {
          "type": "list",
          "position": [ 53, 21 ],
          "columns": 10,
          "callback": "null",
          "scissoring": false,
          "data": { "slotCount": 60 },
          "schema": {
            "spacing": [ 1, 1 ],
            "memberSize": [ 18, 18 ],
            "listTemplate": {
              "slot": {
                "type": "itemslot",
                "showCount": false,
                "showRarity": true,
                "showDurability": true,
                "backingImage": "/interface/inventory/empty.png"
              },
              "count": {
                "type": "label",
                "position": [ 20, -3 ],
                "mouseTransparent": true,
                "scissoring": false,
                "hAnchor": "right"
              }
            }
          }
        },
        "prevPageButton": {
          "type": "button",
          "callback": "Callbacks.changePage",
          "data": -1,
          "position": [ 100, 2 ],
          "pressedOffset": [ -1, 0 ],
          "base": "/pat/infinv/images/buttons/pagearrow.png",
          "hover": "/pat/infinv/images/buttons/pagearrow.png?multiply=afa",
          "pressed": "/pat/infinv/images/buttons/pagearrow.png?multiply=0f0"
        },
        "nextPageButton": {
          "type": "button",
          "callback": "Callbacks.changePage",
          "data": 1,
          "position": [ 140, 2 ],
          "pressedOffset": [ 1, 0 ],
          "base": "/pat/infinv/images/buttons/pagearrow.png?flipx",
          "hover": "/pat/infinv/images/buttons/pagearrow.png?flipx?multiply=afa",
          "pressed": "/pat/infinv/images/buttons/pagearrow.png?flipx?multiply=0f0"
        },
        "pageLabel": {
          "type": "label",
          "position": [ 125, 3 ],
          "hAnchor": "mid"
        },
        "pageScroller": {
          "type": "scrollArea",
          "rect": [ 47, 15, 248, 140 ],
          "verticalScroll": false
        },
        "pageBar": {
          "type": "canvas",
          "rect": [ 47, 15, 248, 17 ],
          "data": { "backColor": "#303030", "color": "#707070" }
        }
      }
    },
    "editorLayout": {
      "type": "layout",
      "layoutType": "basic",
      "scissoring": false,
      "visible": false,
      "children": {
        "settings": {
          "type": "scrollArea",
          "rect": [ 56, 15, 242, 140 ],
          "children": {
            "_anchor": { "type": "widget", "position": [ 0, 15 ] },
            "labelBg": {
              "type": "image",
              "position": [ 1, -3 ],
              "file": "/pat/infinv/images/textbox.png"
            },
            "labelTextbox": {
              "type": "textbox",
              "hint": "^#444;Bag name",
              "position": [ 5, 0 ],
              "maxWidth": 75,
              "callback": "Callbacks.tabLabelTextbox",
              "enterKey": "Callbacks.tabLabelBlur",
              "nextFocus": "_blurTextbox",
              "prevFocus": "_blurTextbox"
            },
            "_blurTextbox": { "type": "textbox", "visible": false, "callback": "Callbacks.tabLabelBlur" },
            "moveTabUpButton": {
              "type": "button",
              "callback": "Callbacks.moveTabButton",
              "data": -1,
              "position": [ 130, -1 ],
              "pressedOffset": [ 0, 0 ],
              "base": "/pat/infinv/images/buttons/arrow.png:up",
              "hover": "/pat/infinv/images/buttons/arrow.png:up?multiply=afa",
              "pressed": "/pat/infinv/images/buttons/arrow.png:upPress?multiply=0f0"
            },
            "moveTabDownButton": {
              "type": "button",
              "callback": "Callbacks.moveTabButton",
              "data": 1,
              "position": [ 142, -1 ],
              "pressedOffset": [ 0, 0 ],
              "base": "/pat/infinv/images/buttons/arrow.png:down",
              "hover": "/pat/infinv/images/buttons/arrow.png:down?multiply=afa",
              "pressed": "/pat/infinv/images/buttons/arrow.png:downPress?multiply=0f0"
            },
            "deleteTabButton": {
              "type": "button",
              "callback": "Callbacks.deleteTabButton",
              "position": [ 156, -2 ],
              "pressedOffset": [ 0, 0 ],
              "base": "/pat/infinv/images/buttons/trash.png:base",
              "hover": "/pat/infinv/images/buttons/trash.png:base?multiply=faa",
              "pressed": "/pat/infinv/images/buttons/trash.png:press?multiply=f00"
            },
            "rotateIconButton": {
              "type": "button",
              "callback": "Callbacks.tabIconRotateButton",
              "position": [ 103, -1 ],
              "pressedOffset": [ 0, 0 ],
              "base": "/pat/infinv/images/buttons/rotate.png:base",
              "hover": "/pat/infinv/images/buttons/rotate.png:base?multiply=aff",
              "pressed": "/pat/infinv/images/buttons/rotate.png:press?multiply=0ff"
            },
            "iconList": {
              "type": "list",
              "position": [ 0, -4 ],
              "columns": 5,
              "fillDown": true,
              "scissoring": false,
              "callback": "Callbacks.tabIconSelected",
              "data": {
                "images": "/pat/infinv/images/tabicons/tabicons.json",
                "iconSlotTemplate": {
                  "type": "itemslot",
                  "position": [ 7, -1 ],
                  "callback": "IconPicker._slotLeft",
                  "rightClickCallback": "IconPicker._slotRight",
                  "mouseTransparent": true,
                  "showCount": false,
                  "showRarity": false,
                  "showDurability": false,
                  "showBackingImageWhenFull": true,
                  "showBackingImageWhenEmpty": true,
                  "backingImage": "/pat/infinv/images/tabiconslot.png"
                }
              },
              "schema": {
                "selectedBG": "/pat/infinv/images/tabselectedbg.png",
                "spacing": [ 2, 3 ],
                "memberSize": [ 32, 16 ],
                "listTemplate": {
                  "background": {
                    "type": "image",
                    "zlevel": -10
                  },
                  "button": {
                    "type": "button",
                    "callback": "null",
                    "position": [ 0, -1 ],
                    "pressedOffset": [ 0, -1 ],
                    "base": "/pat/infinv/images/tabbutton.png:base",
                    "hover": "/pat/infinv/images/tabbutton.png:hover",
                    "pressed": "/pat/infinv/images/tabbutton.png:pressed",
                    "disabledImage": "/pat/infinv/images/tabbutton.png:disabled"
                  },
                  "icon": {
                    "type": "image",
                    "position": [ 16, 8 ],
                    "maxSize": [ 30, 18 ],
                    "zlevel": 2,
                    "centered": true,
                    "mouseTransparent": true
                  }
                }
              }
            }
          }
        },
        "background": {
          "type": "image",
          "position": [ 47, 15 ],
          "zlevel": -100,
          "file": "/pat/infinv/images/tabconfigbg.png",
          "scissoring": false,
          "mouseTransparent": true
        }
      }
    }
  }
}
